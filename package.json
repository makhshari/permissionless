  "private": true,
  "author": "SwipeFi Team",
  "license": "MIT",
  "homepage": "https://swipefi.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/swipefi.git"
  },
  "keywords": [
    "web3",
    "defi",
    "credit-card",
    "blockchain",
    "ethereum",
    "base",
    "nextjs",
    "solidity",
    "typescript"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
